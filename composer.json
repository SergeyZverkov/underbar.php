{
    "name": "emonkak/underbar.php",
    "description": "A collection processing library for PHP, inspired Underscore.js",
    "type": "library",
    "keywords": ["array", "collection", "generator", "iterator", "enumerable"],
    "homepage": "http://emonkak.github.io/underbar.php/",
    "license": "MIT",
    "authors": [
        {
            "name": "Shota Nozaki",
            "email": "emonkak@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.4.0",
      "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Underbar\\": "src/",
            "Underbar\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0-dev"
        }
    }
